class Human {
	int age;
	String name;
}

class Test {
	Human add(Human h) {
		h.age = 100;
		h.name = "홍길동";
		return h;
	}
	
	Human add(Human h, Human h2) {
		h2.age = h.age;
		h2.name = h.name;
		return h2;
	}
}

public class Ex0113_OOP_2 {
	public static void main(String[] args) {
		Test t = new Test();
		Human man = new Human();
		Human woman = new Human();
		
		Human h = t.add(man);
		System.out.println(man.name + "/" + man.age);
		// 이 때 h와 man의 주소는 동일
		// 이 시점에 heap에 올라간 객체는 t, man, woman 세 개이다.
	}
}
